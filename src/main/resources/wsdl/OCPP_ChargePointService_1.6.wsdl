          <s:maxLength value="255"/>
        </s:restriction>
      </s:simpleType>

      <s:simpleType name="CiString500Type">
        <s:annotation>
          <s:documentation>String of maximum 500 printable characters. To be treated as case insensitive.</s:documentation>
        </s:annotation>
        <s:restriction base="s:string">
